{
  "expo": {
    "name": "GymVid",
    "slug": "gymvid-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.gymvid.app",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.gymvid.app"
    },
    "web": {},
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-document-picker",
      "expo-image-picker",
      "expo-av",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow GymVid to use Face ID."
        }
      ]
    ],
    "scheme": "gymvid",
    "jsEngine": "hermes",
    "sdkVersion": "53.0.0"
  }
}
